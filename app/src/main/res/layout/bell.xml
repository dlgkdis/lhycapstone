<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageButton
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/arrow_left"
        android:id="@+id/imageButton56"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:backgroundTint="@color/white"
        android:contentDescription="뒤로가기"
        android:layout_marginTop="40dp"
        app:layout_constraintHorizontal_bias="0.091" />

    <TextView
        android:text="출석보상으로 타코야키 1개 지급 되었습니다."
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nanumfont"
        android:textSize="25dp"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:id="@+id/textView28"
        android:layout_marginTop="108dp"
        app:layout_constraintHorizontal_bias="0.495" />

    <ImageView
        android:layout_width="300dp"
        android:layout_height="35dp"
        app:srcCompat="@drawable/dotted_line_horizontal"
        android:id="@+id/imageView26"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="1dp"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintTop_toBottomOf="@+id/textView28" />

    <ImageView
        android:layout_width="300dp"
        android:layout_height="36dp"
        app:srcCompat="@drawable/dotted_line_horizontal"
        android:id="@+id/imageView27"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="252dp"
        app:layout_constraintHorizontal_bias="0.486"
        app:layout_constraintTop_toBottomOf="@+id/textView29" />

    <ImageView
        android:layout_width="300dp"
        android:layout_height="36dp"
        app:srcCompat="@drawable/dotted_line_horizontal"
        android:id="@+id/imageView28"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="312dp"
        app:layout_constraintHorizontal_bias="0.486" />

    <ImageView
        android:layout_width="300dp"
        android:layout_height="36dp"
        app:srcCompat="@drawable/dotted_line_gray"
        android:id="@+id/imageView29"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="196dp"
        app:layout_constraintHorizontal_bias="0.504" />

    <TextView
        android:text="상대방이 책장 오브제를 구매했습니다."
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nanumfont"
        android:textSize="25dp"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:id="@+id/textView29"
        android:layout_marginTop="4dp"
        app:layout_constraintHorizontal_bias="0.411"
        app:layout_constraintTop_toBottomOf="@+id/imageView26" />

    <TextView
        android:text="하루일기 작성으로 타코야키 \n2알 지급되었습니다."
        android:layout_width="290dp"
        android:layout_height="80dp"
        android:fontFamily="@font/nanumfont"
        android:textSize="25dp"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:id="@+id/textView30"
        android:layout_marginTop="232dp"
        app:layout_constraintHorizontal_bias="0.446" />

    <TextView
        android:text="하루일기를 작성해 기록을 남기고 \n타코야키를 받아보세요! "
        android:layout_width="281dp"
        android:layout_height="74dp"
        android:fontFamily="@font/nanumfont"
        android:textSize="25dp"
        android:textColor="#6E6D6D"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:id="@+id/textView31"
        android:layout_marginTop="388dp"
        app:layout_constraintHorizontal_bias="0.43" />

</androidx.constraintlayout.widget.ConstraintLayout>