<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.dashboard.DashboardFragment">

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/nav_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="?android:attr/windowBackground"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:menu="@menu/bottom_nav_menu" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="392dp"
        android:layout_height="712dp"
        android:src="@drawable/tema"
        tools:layout_editor_absoluteX="9dp"
        tools:layout_editor_absoluteY="30dp" />

    <ImageButton
        android:layout_width="24dp"
        android:layout_height="wrap_content"
        android:background="@drawable/tapbar_store"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="62dp" />

    <ImageButton
        android:layout_width="24dp"
        android:layout_height="wrap_content"
        android:background="@drawable/tapbar_reward"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="88dp"
        tools:layout_editor_absoluteY="62dp" />

    <ImageButton
        android:layout_width="24dp"
        android:layout_height="wrap_content"
        android:background="@drawable/tapbar_bell"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="61dp" />

    <ImageButton
        android:layout_width="24dp"
        android:layout_height="wrap_content"
        android:background="@drawable/tapbar_person"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="64dp"
        tools:layout_editor_absoluteY="62dp" />

</androidx.constraintlayout.widget.ConstraintLayout>